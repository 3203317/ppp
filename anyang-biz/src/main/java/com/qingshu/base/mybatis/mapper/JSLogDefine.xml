<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="JSLogDefine">
	<resultMap type="JSLogDefine" id="jSLogDefineResultMap">
		<result property="id" column="id" />
	</resultMap>
	
	<sql id="table_columns">
		id,tableDesc,pojoName,findPojoName,findMethod,methodName,logDesc
    </sql>
	<sql id="entity_properties">
		#{id,jdbcType=VARCHAR},#{tableDesc,jdbcType=VARCHAR},#{pojoName,jdbcType=VARCHAR},#{findPojoName,jdbcType=VARCHAR},#{findMethod,jdbcType=VARCHAR},#{methodName,jdbcType=VARCHAR},#{logDesc,jdbcType=VARCHAR}
	</sql>
	
	<!-- 适用于主键自增类型 -->
	<insert id="save" parameterType="JSLogDefine">
		insert into j_s_logdefine( <include refid="table_columns" /> ) 
		values ( <include refid="entity_properties" /> )
	</insert>
	<!-- 批量保存对象 -->
	<insert id="batchInsert" parameterType="list">
		insert into
		j_s_logdefine
		(
		<include refid="table_columns" />
		)
		<foreach collection="list" item="item" index="index"
			separator="union all">
			select #{item.id},#{item.tableDesc},#{item.pojoName},#{item.findPojoName},#{item.findMethod},#{item.methodName},#{item.logDesc} from dual
		</foreach>
	</insert>
	<delete id="deleteByIds">
		delete from j_s_logdefine
		where id in
		<foreach item="item" collection="array" open="(" separator="," close=")">
			#{item}
		</foreach>
	</delete>
	<delete id="deleteAll">
		delete from j_s_logdefine
	</delete>
	<update id="update" parameterType="JSLogDefine">
		update j_s_logdefine 
		<trim prefix="set" suffixOverrides=",">
		</trim>
		<where>id = #{id}</where>
	</update>
	
	<select id="findById" resultMap="jSLogDefineResultMap" parameterType="String" >
		select <include refid="table_columns" />
		from j_s_logdefine
		where id = #{id}
	</select>
	<select id="findByPojoName" resultType="JSLogDefine" parameterType="Map" >
		select <include refid="table_columns" />
		from j_s_logdefine
		where pojoName = #{pojoName} and methodName=#{methodName}
	</select>
	<select id="queryAll" resultMap="jSLogDefineResultMap">
		select <include refid="table_columns" /> 
		from j_s_logdefine
	</select>
	
	<!-- 使用like用法：columnName like concat('%',#columnName#,'%') -->
	<sql id="page_where">
		<trim prefix="where" prefixOverrides="and | or ">
		</trim>
	</sql>
	
	<!-- 获取对象列表(分页)-->
	<select id="pagerList" resultMap="jSLogDefineResultMap" parameterType="PagerInfo">
		select <include refid="table_columns" />
		from j_s_logdefine
		<include refid="page_where" />
	</select>
	
	<!-- 其他自定义SQL -->
	
</mapper>