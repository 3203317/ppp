<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="JSLogDetail">
	<resultMap type="JSLogDetail" id="jSLogDetailResultMap">
		<result property="id" column="id" />
		<association property="jsLog" column="logId" javaType="JSLog" select="JSLog.findById" />
	</resultMap>
	
	<sql id="table_columns">
		id,logId,fieldName,newValue,oldValue,fieldDesc
    </sql>
	<sql id="entity_properties">
		#{id,jdbcType=VARCHAR},#{jsLog.id,jdbcType=VARCHAR},#{fieldName,jdbcType=VARCHAR},#{newValue,jdbcType=VARCHAR},#{oldValue,jdbcType=VARCHAR},#{fieldDesc,jdbcType=VARCHAR}
	</sql>
	
	<!-- 适用于主键自增类型 -->
	<insert id="save" parameterType="JSLogDetail">
		insert into j_s_logdetail( <include refid="table_columns" /> ) 
		values ( <include refid="entity_properties" /> )
	</insert>
	<!-- 批量保存对象 -->
	<insert id="batchInsert" parameterType="list">
		insert into
		j_s_logdetail
		(
		<include refid="table_columns" />
		)
		<foreach collection="list" item="item" index="index"
			separator="union all">
			select #{item.id},#{item.jsLog.id},#{item.fieldName},#{item.newValue},#{item.oldValue},#{item.fieldDesc} from dual
		</foreach>
	</insert>
	<delete id="deleteByIds">
		delete from j_s_logdetail
		where id in
		<foreach item="item" collection="array" open="(" separator="," close=")">
			#{item}
		</foreach>
	</delete>
	
	<update id="update" parameterType="JSLogDetail">
		update j_s_logdetail 
		<trim prefix="set" suffixOverrides=",">
		</trim>
		<where>id = #{id}</where>
	</update>
	
	<select id="findById" resultMap="jSLogDetailResultMap" parameterType="String" >
		select <include refid="table_columns" />
		from j_s_logdetail
		where id = #{id}
	</select>
	
	<select id="queryAll" resultMap="jSLogDetailResultMap">
		select <include refid="table_columns" /> 
		from j_s_logdetail
	</select>
	
	<!-- 使用like用法：columnName like concat('%',#columnName#,'%') -->
	<sql id="page_where">
		<trim prefix="where" prefixOverrides="and | or ">
		</trim>
	</sql>
	
	<!-- 获取对象列表(分页)-->
	<select id="pagerList" resultMap="jSLogDetailResultMap" parameterType="PagerInfo">
		select <include refid="table_columns" />
		from j_s_logdetail
	</select>
	
	<!-- 其他自定义SQL -->
	
</mapper>